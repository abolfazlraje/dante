EOF

fi

	#third argument
	
_arg=3
_param=conftest.out_param_3
_func=$func
_ucfunc=$ucfunc

unset _nofunc

for val in "socklen_t" "int" "size_t"
do
	cat ${_param} | egrep "^${val}" > /dev/null
	test $? -eq 0 && _nofunc="" && break
	_nofunc=t
done
if test "x${_nofunc}" = xt; then
	if test "x$failure" != x; then
		failure="$failure|"
	fi
	failure="$failure ${_func} (${_arg}): (`cat ${_param}`)"
else
	cat >> confdefs.h <<EOF
